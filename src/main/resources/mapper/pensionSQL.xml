<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="pension">
	<insert id="insertPension" parameterType="pension">
		insert into pension_tbl
		values(pension_seq.nextval, #{ownerNo}, #{pensionName}, #{pensionAddr}, #{pensionSerivce}, #{roomCnt}, #{pensionImg}, #{businessLicenseNo})
	</insert>
</mapper>
